{
	"name": "eslint-plugin-tsdoc",
	"version": "0.2.7",
	"description": "An ESLint plugin that validates TypeScript doc comments",
	"keywords": [
		"TypeScript",
		"documentation",
		"doc",
		"comments",
		"JSDoc",
		"TSDoc",
		"ESLint",
		"plugin"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/tsdoc/tree/master/eslint-plugin"
	},
	"homepage": "https://github.com/microsoft/tsdoc",
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"scripts": {
		"build": "heft test --clean",
		"lint": "eslint -f unix \"src/**/*.{ts,tsx}\""
	},
	"dependencies": {
		"@microsoft/tsdoc": "0.12.21",
		"@microsoft/tsdoc-config": "0.13.6"
	},
	"devDependencies": {
		"@microsoft/rush-stack-compiler-3.5": "~0.8.4",
		"@rushstack/eslint-config": "~1.3.0",
		"@rushstack/heft": "^0.10.1",
		"@types/eslint": "6.1.3",
		"@types/estree": "0.0.39",
		"@types/heft-jest": "1.0.1",
		"@types/node": "10.17.5",
		"eslint": "~7.2.0"
	}
}
